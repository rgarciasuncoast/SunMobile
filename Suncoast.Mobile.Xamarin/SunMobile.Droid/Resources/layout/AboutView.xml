<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="@color/white"
    android:fitsSystemWindows="true"
    android:orientation="vertical">
    <WebView
        android:layout_width="match_parent"
        android:layout_height="200dp"
        android:id="@+id/webViewAbout"
        android:layout_marginLeft="10dp"
        android:scrollbars="none" />
    <ScrollView
        android:minWidth="25px"
        android:minHeight="25px"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/scrollView1"
        android:scrollbars="none"
        android:background="@color/white">
        <LinearLayout
            android:orientation="vertical"
            android:minWidth="25px"
            android:minHeight="25px"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@color/white"
            android:fitsSystemWindows="true"
            android:id="@+id/linearLayout1">
            <TableLayout
                android:id="@+id/mainTable"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:stretchColumns="3">
                <View
                    android:layout_span="5"
                    android:layout_width="match_parent"
                    android:layout_height="1dip"
                    android:layout_weight="1"
                    android:background="@drawable/default_separatorcolor"
                    android:padding="2dip"
                    android:layout_marginTop="5dp" />
                <TableRow
                    android:id="@+id/WebSiteRow"
                    android:clickable="true"
                    android:focusable="true"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/default_background"
                    android:layout_span="5"
                    android:gravity="center_vertical"
                    android:paddingTop="6dp"
                    android:paddingBottom="6dp">
                    <ImageView
                        android:src="@drawable/iconsmall50"
                        android:layout_width="48dp"
                        android:layout_height="48dp"
                        android:id="@+id/imageView1"
                        android:layout_column="0"
                        android:layout_span="1"
                        android:layout_marginLeft="5dp" />
                    <TextView
                        android:text="SuncoastCreditUnion.com"
                        android:id="@+id/lblWebsite"
                        android:textSize="@dimen/TextSizeLarge"
                        android:textColor="@color/TextViewTextColor"
                        android:typeface="sans"
                        android:textStyle="normal"
                        android:layout_column="2"
                        android:layout_span="1"
                        android:paddingLeft="5dp" />
                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:src="@drawable/listitemselect"
                        android:paddingRight="2dp"
                        android:layout_column="4"
                        android:layout_span="1" />
                </TableRow>
                <View
                    android:layout_span="5"
                    android:layout_width="match_parent"
                    android:layout_height="1dip"
                    android:layout_weight="1"
                    android:background="@drawable/default_separatorcolor"
                    android:padding="2dip"
                    android:layout_marginTop="5dp" />
                <TableRow
                    android:id="@+id/FacebookRow"
                    android:clickable="true"
                    android:focusable="true"
                    android:background="@drawable/default_background"
                    android:layout_span="5"
                    android:gravity="center_vertical"
                    android:paddingTop="6dp"
                    android:paddingBottom="6dp">
                    <ImageView
                        android:src="@drawable/facebook"
                        android:layout_width="48dp"
                        android:layout_height="48dp"
                        android:id="@+id/imageViewFacebook"
                        android:layout_column="0"
                        android:layout_span="1"
                        android:layout_marginLeft="5dp" />
                    <TextView
                        android:text="Suncoast Facebook Page"
                        android:id="@+id/lblFacebook"
                        android:textSize="@dimen/TextSizeLarge"
                        android:textColor="@color/TextViewTextColor"
                        android:typeface="sans"
                        android:textStyle="normal"
                        android:layout_column="2"
                        android:layout_span="1"
                        android:paddingLeft="5dp" />
                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:src="@drawable/listitemselect"
                        android:paddingRight="2dp"
                        android:layout_column="4"
                        android:layout_span="1" />
                </TableRow>
                <View
                    android:layout_span="5"
                    android:layout_width="match_parent"
                    android:layout_height="1dip"
                    android:layout_weight="1"
                    android:background="@drawable/default_separatorcolor"
                    android:padding="2dip"
                    android:layout_marginTop="5dp" />
                <TableRow
                    android:id="@+id/TwitterRow"
                    android:clickable="true"
                    android:focusable="true"
                    android:background="@drawable/default_background"
                    android:layout_span="5"
                    android:gravity="center_vertical"
                    android:paddingTop="6dp"
                    android:paddingBottom="6dp">
                    <ImageView
                        android:src="@drawable/twitter_flat_icon"
                        android:layout_width="48dp"
                        android:layout_height="48dp"
                        android:id="@+id/imageViewTwitter"
                        android:layout_column="0"
                        android:layout_span="1"
                        android:layout_marginLeft="5dp" />
                    <TextView
                        android:text="Suncoast Twitter Page"
                        android:id="@+id/lblTwitter"
                        android:textSize="@dimen/TextSizeLarge"
                        android:textColor="@color/TextViewTextColor"
                        android:typeface="sans"
                        android:textStyle="normal"
                        android:layout_column="2"
                        android:layout_span="1"
                        android:paddingLeft="5dp" />
                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:src="@drawable/listitemselect"
                        android:paddingRight="2dp"
                        android:layout_column="4"
                        android:layout_span="1" />
                </TableRow>
                <View
                    android:layout_span="5"
                    android:layout_width="match_parent"
                    android:layout_height="1dip"
                    android:layout_weight="1"
                    android:background="@drawable/default_separatorcolor"
                    android:padding="2dip"
                    android:layout_marginTop="5dp" />
                <TableRow
                    android:id="@+id/VersionRow"
                    android:clickable="true"
                    android:focusable="true"
                    android:background="@drawable/default_background"
                    android:layout_span="5"
                    android:gravity="center_vertical"
                    android:paddingTop="18dp"
                    android:paddingBottom="18dp">
                    <TextView
                        android:id="@+id/lblVersion"
                        android:textSize="@dimen/TextSizeLarge"
                        android:textColor="@color/TextViewTextColor"
                        android:typeface="sans"
                        android:textStyle="normal"
                        android:layout_column="2"
                        android:layout_span="1"
                        android:paddingLeft="5dp" />
                </TableRow>
                <View
                    android:layout_span="5"
                    android:layout_width="match_parent"
                    android:layout_height="1dip"
                    android:layout_weight="1"
                    android:background="@drawable/default_separatorcolor"
                    android:padding="2dip"
                    android:layout_marginTop="5dp" />
            </TableLayout>
        </LinearLayout>
    </ScrollView>
</LinearLayout>